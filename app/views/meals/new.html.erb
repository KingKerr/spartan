<nav class="navbar navbar-inverse">
  <h1>Meals</h1>
</nav>
<div class="col-md-6">
  <h3>Protein</h3>
    <% simple_form_for([current_user, @meal] ) do |f| %>
    <%= f.select :name, @protein_options %>
    <% hidden_field :type, "protein" %>
    <% f.submit %>
    <% end %>
</div>

<div class="col-md-6">
  <h3>Sides</h3>
  <% simple_form_for([current_user, @meal] ) do |f| %>
  <%= f.select :name, @side_options %>
  <% hidden_field :type, "sides" %>
  <% f.submit %>
  <% end %>
</div>
